		function retrieve(gubn){
			switch(gubn){
				case "search1" :
				case "search2" :
				case "search3" :
				case "search4" :
					doSearch(selectNum(gubn));
					break;
				case "save1" :
				case "save2" :
				case "save3" :
				case "save4" :
					doInsert(selectNum(gubn));
					break;
				case "delete1" :
				case "delete2" :
				case "delete3" :
				case "delete4" :
					doDelete(selectNum(gubn));
					break;
				case "add1" :
				case "add2" :
				case "add3" :
				case "add4" :
					addRow(selectNum(gubn));
					break;
			}
		}

