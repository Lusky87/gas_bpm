					$("#tmp_cat_1").val(getColValue("a_catg_3",1, "htmlTable2"));
					previSRow1 = 1;
					jQuery("#"+gridName2).setRowData (1,false,{background:'#B2EBF4'});
					doSearch(3);
				}
				break;
			case "3" :
				setChangeImg();
