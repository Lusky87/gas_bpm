.paging {
    text-align: center; vertical-align: top;
}
.paging a {
    background: rgb(255, 255, 255); border: 1px solid rgb(182, 194, 217); width: 22px; height: 22px; text-align: center; color: rgb(51, 51, 51); line-height: 22px; font-size: 11px; margin-left: 3px; vertical-align: top; display: inline-block;
}
.paging a.on {
    background: rgb(142, 164, 207); border: 1px solid rgb(112, 135, 180); color: rgb(255, 255, 255);
}
.paging a span {
    top: -9999px; text-indent: -9999px; position: absolute;
}
.paging a.first {
    margin-left: 20px;
}
.paging a.last {
    margin-right: 20px;
}
.paging a.btn_first {
    background: url("/images/hncis_common/btn_paging.gif") no-repeat left top;
