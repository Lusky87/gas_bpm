				parent.$("#doc_no").val(getColValue("doc_no", 1));
				parent.$("#hid_doc_no").val(getColValue("doc_no", 1));
			}
			parent.$("#snb_rson_sbc").val(getColValue("snb_rson_sbc", 1));

