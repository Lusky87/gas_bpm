@charset "utf-8";
/* CSS Document */
@import url(reset.css);
#e_book_wrap { width: 1135px; height: 900px; }
.e_book_header { height: 70px; background: url(../../images/e_book/e_book_header_bg.png) top left no-repeat; }
.e_book_header input { width: 39px; height: 23px; padding: 0 0 0 10px; margin-left: 13px; font-size: 14px; }
.e_book_logo_area { float: left; padding-top: 7px; margin-left: 15px;  }
.e_book_logo_area h1, .e_book_logo_area .e_book_tit { float: left; }
.e_book_logo_area .e_book_tit { margin-left: 13px; }
.e_book_adjust_area { float: right; padding-top: 17px; margin-right: 30px; font-size: 0px; }
.e_book_adjust_area a, .e_book_adjust_area span { display: inline-block; }
.e_book_adjust_area span { margin-left: 8px; margin-right: 11px; font-family: 'NanumGothic', '나눔고딕','Nanum Gothic', sans-serif; font-weight: 800; font-size: 20px; color: #fff; vertical-align: middle; }
.e_book_adjust_area a.e_prav_btn, .e_book_adjust_area a.e_next_btn { margin-left: 3px; margin-right: 3px; }
.e_book_logo_area:after,
.e_book_header:after { display: block; width: 0px; height: 0px; content:''; clear: both; }
.e_book_con { padding-top: 30px; text-align: center; }
.e_book_tit {color:#fff; font-size:36px;}
