html,body,h1,h2,h3,h4,h5,h6,p,div,th,td,table,pre,code,select,form,fieldset,legend,input,address,ol,ul,li,dl,dt,dd,img,textarea,blockquote,em{margin:0; padding:0;}
em{font-style:normal;}
body {font-family:"Nanum Gothic",Helvetica, Arial,Verdana; font-size:11px;}
a{text-decoration:none;}
li{list-style:none;}
img{border:none;}
h1,h2,h3,h4,h5,h6{font-weight:normal;}
input, select{box-sizing:border-box; -moz-box-sizing:border-box;}
input.disabled{background-color:#e2e2e2; border: 1px solid #bebebe; color:#222 !important;}
.tbl_fixed {table-layout: fixed;}
.bor_collapse {border-collapse: collapse;}
.clear{clear:both;}
.t_center{text-align: center;}
.fl{float:left !important;}
.fr{float:right !important;}
.clear{clear:both;}
.mt0{margin-top:0px !important;}
.mt10{margin-top:80px !important;}
.mt20{margin-top:20px !important;}
.mt30{margin-top:30px !important;}
.ml5{margin-left:5px !important;}
.ml10{margin-left:10px !important;}
.ml20{margin-left:20px !important;}
.pt10{padding-top:10px !important;}
.pt20{padding-top:20px !important;}
.pt30{padding-top:30px !important;}
.pb30{padding-bottom:30px !important;}

.pic_div { width:500px; height: 139px; background-color: #ffffff; position: absolute; z-index: 1200; top: 30%; left: 20%; }
.pic_div1 { width:500px; height: 139px; background-color: #ffffff; position: absolute; z-index: 1200; top: 40%; left: 45%; }
.pic_div2 { width:500px; height: 139px; background-color: #ffffff; position: absolute; z-index: 1200; top: 30%; left: 40%; }
.div_border { border: 2px #bbc3cb solid; }

.view_pic_div { width:250px; height: 230px; background-color: #ffffff; position: absolute; z-index: 1200; top: 30%; left: 20%; }
.view_pic_div1 { width:250px; height: 230px; background-color: #ffffff; position: absolute; z-index: 1200; top: 40%; left: 45%; }
.view_div_border { border: 2px #bbc3cb solid; }

#contents div.list{display:inline-block;clear:both; margin:10px 0; height:190px;}
#contents div.good:first-child{float:left; border:1px solid #e9e9e9; border-top:3px solid #6a8ec6;}
#contents div.good{float:left; margin:0 0 0 24px; border:1px solid #e9e9e9; border-top:3px solid #6a8ec6; font-size:13px;}
#contents div.good li{padding:5px 0;}
#contents div.good li.img{padding:0;}
#contents div.good li.title{padding-left:5px; color:#1469d2;}
#contents div.good li.sub{padding-left:5px; color:#8b8b8b;}
#contents div.good li.cont{padding-left:5px; color:#; cursor : pointer;}

.paging {
    text-align: center; vertical-align: top;
}
.paging a {
    background: rgb(255, 255, 255); border: 1px solid rgb(182, 194, 217); width: 22px; height: 22px; text-align: center; color: rgb(51, 51, 51); line-height: 22px; font-size: 11px; margin-left: 3px; vertical-align: top; display: inline-block;
}
.paging a.on {
    background: rgb(142, 164, 207); border: 1px solid rgb(112, 135, 180); color: rgb(255, 255, 255);
}
.paging a span {
    top: -9999px; text-indent: -9999px; position: absolute;
}
.paging a.first {
    margin-left: 20px;
}
.paging a.last {
    margin-right: 20px;
}
.paging a.btn_first {
    background: url("/images/hncis_common/btn_paging.gif") no-repeat left top;
}
.paging a.btn_prev {
    background: url("/images/hncis_common/btn_paging.gif") no-repeat left bottom;
}
.paging a.btn_next {
    background: url("/images/hncis_common/btn_paging.gif") no-repeat right bottom;
}
.paging a.btn_last {
    background: url("/images/hncis_common/btn_paging.gif") no-repeat right top;
}